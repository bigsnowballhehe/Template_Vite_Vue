{
  "extends": ["@antfu"],
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        "no-console": "off",
        "no-alert": "off",
        "vue/component-tags-order": [
          "error",
          {
            "order": [["template", "script"], "style"]
          }
        ]
      }
    },
    {
      "files": ["*.ts", "*.js"],
      "rules": {
        "no-console": "off",
        "no-alert": "off"
      }
    }
  ]
}
